    "rollup-plugin-visualizer": "^5.9.2",
    "rollup-plugin-webpack-stats": "^0.2.0",
    "storybook": "^7.4.3",
    "storybook-docs-toc": "^1.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3"
  },
  "peerDependencies": {
    "antd": "^5.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-modal": "^3.16.1",
