  "homepage": "https://www.aristid.com",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "build": "tsc && vite build -c vite.config.cjs.ts && vite build -c vite.config.ts --emptyOutDir false",
    "build-stats": "STATS=true tsc && vite build",
    "build-storybook": "env=production storybook build --docs",
    "build-all": "yarn build && yarn build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes",
    "copy-typescript-definitions": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "dev": "vite",
    "commit": "cz",
    "generate-tokens": "node tokens-generator.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test-build": "yarn lint && yarn build"
  },
